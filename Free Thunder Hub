local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
	Name = "Thunder Hub loader",
	LoadingTitle = "Script Loader",
	LoadingSubtitle = "Script By Tili",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
        Discord = {
        	Enabled = false,
        	Invite = " no ", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "Thunder Hub",
		Subtitle = "Key System",
		Note = "You agree with TOS by using script",
		FileName = "Thunder Key",
		SaveKey = false,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "lajsdjiahudwawhduawhusdha"
	}
})
Rayfield:Notify({
    Title = "Thanks for Using script",
    Content = "Thunder Hub on top",
    Duration = 10,
    Image = 4483362458,
    Actions = { -- Notification Buttons
        Ignore = {
            Name = "Okay!",
            Callback = function()
                print("The user tapped Okay!")
            end
		},
	},
})
local Tab = Window:CreateTab("ScriptLoader", 4483362458) -- Title, Image
local Section = Tab:CreateSection("Scriptloader")
local Button = Tab:CreateButton({
	Name = "Load Script here",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Tilitestaccount/Idk/main/ThunderHubV1", true))()
	end,
})
local Button = Tab:CreateButton({
	Name = "Destroy this GUI here",
	Callback = function()
		Rayfield:Destroy()
	end,
})
